{
  "name": "turiknox/module-linkguestorders",
  "description": "A Magento 2 module that checks if newly registered customers have made orders in the past as a guest. If so, the orders are assigned to their account.",
  "version": "1.0.0",
  "license": "MIT",
  "require": {
    "php": "~7.0.2|7.0.4|~7.0.6|~7.1.0",
    "magento/module-backend": "100.1.*",
    "magento/module-store": "100.1.*",
    "magento/framework": "100.1.*"
  },
  "type": "magento2-module",
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/Turiknox/magento2-linkguestorders"
    }
  ],
  "autoload": {
    "files": [
      "registration.php"
    ],
    "psr-4": {
      "Turiknox\\LinkGuestOrders\\": ""
    }
  }
}
